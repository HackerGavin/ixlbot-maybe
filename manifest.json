{
  "manifest_version": 3,
  "name": "AI Answer Finder",
  "version": "1.0",
  "description": "Scan questions on a page and provide answers using predefined rules.",
  "permissions": ["activeTab", "scripting"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  }
}
